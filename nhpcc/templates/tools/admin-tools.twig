{%- macro getRole(level) -%}
{% if level >= 15 %}
管理员
{% elseif level >= 9 %}
超级用户
{% elseif level >= 5 %}
教师
{% elseif level >= 3 %}
学生
{% else %}
普通用户
{% endif %}
{%- endmacro -%}

{%- macro getArticleType(num) -%}
{% if num == 1 %}
新闻简报
{% elseif num == 5 %}
通知公告
{% elseif num == 9 %}
学术活动
{% endif %}
{%- endmacro -%}
